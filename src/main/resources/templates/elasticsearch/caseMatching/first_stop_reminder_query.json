{
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "last_modified": {
              "gte": "%s",
              "lte": "%s"
            }
          }
        },
        {
          "terms": {
            "state.keyword": [
              "BOCaseStopped",
              "BOCaseStoppedReissue"
            ]
          }
        }
      ],
      "must_not": [
        {
          "terms": {
            "data.caseType.keyword": [
              "edgeCase"
            ]
          }
        },
        {
          "terms": {
            "data.evidenceHandled": [
              "No"
            ]
          }
        },
        {
          "terms": {
            "data.boCaseStopReasonList.value.caseStopReason.keyword": [
              "CaveatMatch",
              "Permanent Caveat"
            ]
          }
        }
      ]
    }
  },
  "size": %s,
  "_source": ["reference"],
  "sort": [
    {
      "reference.keyword": "asc"
    }
  ]
