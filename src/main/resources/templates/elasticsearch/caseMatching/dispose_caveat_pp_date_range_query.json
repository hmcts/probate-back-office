{
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "last_modified": {
              "gte": "%s",
              "lte": "%s"
            }
          }
        },
        {
          "match": {
            "data.applicationType.keyword": "Solicitor"
          }
        },
        {
          "terms": {
            "state.keyword": [
              "SolAppCreated",
              "SolAppUpdated",
              "CasePaymentFailed"
            ]
          }
        }
      ]
    }
  },
  "size": %s,
  "sort": [
    {
      "reference.keyword": "asc"
    }
  ]