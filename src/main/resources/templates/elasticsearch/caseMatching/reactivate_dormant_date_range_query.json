{
  "size": :size,
  "from": :from,
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "last_modified": {
              "gte": ":fromDate"
            }
          }
        },
        {
          "match": {
            "state": "Dormant"
          }
        }
      ]
    }
  },
  "sort": [
    {
      "id": {
        "order": "asc"
      }
    }
  ]
}