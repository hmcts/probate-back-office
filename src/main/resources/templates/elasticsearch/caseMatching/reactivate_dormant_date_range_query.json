{
  "size": :size,
  "from": :from,
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "last_modified": {
              "gte": ":fromDate",
              "lte": ":toDate"
            }
          }
        },
        {
          "script": {
            "script": "if (doc['last_modified'].size() > 0 && doc['data.movedIntoDormantDateTime'].size() > 0) { return doc['last_modified'].value.millis > doc['data.movedIntoDormantDateTime'].value.millis } return false;"
          }
        },
        {
          "match": {
            "state": "Dormant"
          }
        }
      ]
    }
  },
  "sort": [
    {
      "id": {
        "order": "asc"
      }
    }
  ]
}