{
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "last_modified": {
              "gte": "%s",
              "lte": "%s"
            }
          }
        },
        {
          "terms": {
            "state.keyword": [
              "Deleted",
              "Stopped"
            ]
          }
        }
      ]
    }
  },
  "sort": [
    {
      "id": {
        "order": "asc"
      }
    }
  ]