{
  "size": :size,
  "from": :from,
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "last_modified": {
              "gte": ":fromDate",
              "lte": ":toDate"
            }
          }
        },
        {
          "terms": {
            "state.keyword": [
              "Deleted"
            ]
          }
        }
      ]
    }
  },
  "sort": [
    {
      "id": {
        "order": "asc"
      }
    }
  ]
}