{
  "size": :size,
  "from": 0,
  "query": {
    "bool": {
      "filter": [
        {
          "match": {
            "state": "BOGrantIssued"
          }
        },
        {
          "range": {
            "data.grantIssuedDate": {
              "gte": ":fromDate",
              "lte": ":toDate"
            }
          }
        },
        {
          "exists": {
            "field": "data.scannedDocuments"
          }
        }
      ]
    }
  }
}